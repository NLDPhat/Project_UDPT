					<label for="name">Import:</label>
					<textarea class="form-control" rows="7" id="content" placeholder="Write your email"></textarea>
				</div>
				<div>
					<button type="button" class="btn btn-default" id="send_email">Send Email</button>
				</div>
			</form>
		</div>
	</div>
</body>
</html>
